<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suites" verbose="1" >
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.tutorialsninja.demo.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Login Test" >
        <classes>
            <class name="com.tutorialsninja.demo.testsuite.TopMenuTest"/>
            <class name="com.tutorialsninja.demo.testsuite.DesktopsTest"/>
            <class name="com.tutorialsninja.demo.testsuite.LaptopsAndNotebooksPageTest"/>
            <class name="com.tutorialsninja.demo.testsuite.MyAccountsTest"></class>
        </classes>
    </test>
</suite>